{
    "name": "gcs-radio",
    "version": "1.17.11",
    "description": "",
    "main": "src/backend/index.ts",
    "scripts": {
        "start": "node build/backend/index.js",
        "dev:full": "npm-run-all --parallel watch dev",
        "dev": "nodemon -e ts --exec \"npm run build:backend:serve\"",
        "watch": "webpack --config src/webpack.config.js --watch",
        "build:production": "sed -i 's/development/production/' src/webpack.config.js ; npm run build ; sed -i 's/production/development/' src/webpack.config.js ;",
        "build:frontend": "webpack --config src/webpack.config.js",
        "build:backend": "tsc",
        "build:backend:serve": "npm run build:backend && node build/backend/index.js",
        "build": "npm run build:frontend && npm run build:backend",
        "clean": "rimraf build",
        "rebuild": "npm run build",
        "prerebuild": "npm run clean",
        "prebuild": "npm run lint",
        "test": "jest --colors --verbose --silent",
        "test:loud": "jest --colors --verbose",
        "lint": "eslint \"src/**/*.[jt]s\" --fix",
        "clearCache": "mongo \"$( cat .env | grep DB_CONNECTION | cut -d'=' -f2 )\" < src/dev/clearCache.js",
        "clearUsers": "mongo \"$( cat .env | grep DB_CONNECTION | cut -d'=' -f2 )\" < src/dev/clearUsers.js",
        "deploy": "npm run lint && bash ./src/dev/deploy.sh gcs-radio",
        "deploy:beta": "npm run lint && bash ./src/dev/deploy.sh gcs-radio-beta",
        "generateDefaultAudio": "npm run build:backend && ./src/dev/generateDefaultAudio.sh",
        "generateDefaultAudio:noBuild": "./src/dev/generateDefaultAudio.sh",
        "generateVoiceTemplates": "npm run build:backend && ./src/dev/generateVoiceTemplates.sh",
        "generateVoiceTemplates:noBuild": "./src/dev/generateVoiceTemplates.sh",
        "dockerize": "sh ./src/dev/dockerize.sh"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/brennanwilkes/gcs-radio.git"
    },
    "author": "Brennan Wilkes <brennan@codexwilkes.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/brennanwilkes/gcs-radio/issues"
    },
    "homepage": "https://gitlab.com/brennanwilkes/gcs-radio#readme",
